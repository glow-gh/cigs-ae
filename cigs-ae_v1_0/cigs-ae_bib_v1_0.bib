@article{petrieDevelopingSocietiesEconomies2007,
	title = {Developing {Societies} and {Economies} in {Fourth} {Millennium} {B}.{C}. {Fars}: {Further} {Excavations} at {Tol}-e {Spid}},
	volume = {45},
	issn = {0578-6967},
	shorttitle = {Petrie et al. 2007},
	url = {https://www.jstor.org/stable/25651425},
	abstract = {Excavations were resumed at the site of Tol-e Spid in the Mamasani District of Fars in early 2007. Previous excavations in 2003 revealed a deep sequence of occupation but halted due to time constraints. At that time, the lowest phases exposed dated to the fourth millennium B.C. and were characterised by Lapui period ceramics. The 2007 excavations have shown that the sequence of Lapui period deposits continues at least 4 m. below the modern level of the plain. These phases await absolute dating, but it is possible that Tol-e Spid has a Lapui period sequence that commences before 4000 B.C. and occupation that continues throughout the period of critical socio-economic developments that characterise the fourth millennium B.C.},
	urldate = {2021-02-15},
	journal = {Iran},
